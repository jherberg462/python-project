    "# Groupby trending_df by country and sum the views.\n",
    "views_country_groups = trending_df.groupby(\"Country\").sum()\n",
    "\n",
    "# Get number of views on millions for better visualization\n",
    "views_million = views_country_groups['Views']/1000000000\n",
    "views_country_groups['Views (billions)'] = views_million\n",
    "\n",
    "# Convert series to DataFrame\n",
    "views_country_df = pd.DataFrame(views_country_groups)\n",
    "\n",
    "# Add population columns and data\n",
    "population = [37074562, 83124418, 64990511,67141684, 1352642280,\n",
    "             127202192, 51171706, 126190788, 145734038, 327096265]\n",
    "views_country_df[\"Population\"] = population\n",
    "\n",
    "# Get values for the per capita column and add it to the DataFrame\n",
    "per_capita = (views_country_df['Views']/views_country_df['Population'])\n",
    "\n",
    "views_country_df['Views Per Capita'] = per_capita\n",
    "\n",
    "# Preview of the DataFrame\n",
    "views_country_df.sort_values(by=['Views (billions)'])"
   ]
